{
  "name": "kitaa-root",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "vercel-build": "node scripts/build_public.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel deploy --prebuilt",
    "deploy:prod": "vercel deploy --prebuilt --prod",
    "build:public": "node scripts/build_public.js",
    "build": "npm run build:public",
    "preaudit:css": "npm run build:public",
    "audit:css": "node scripts/css-audit.js",
    "serve:ci": "node scripts/serve_static_ci.js",
    "check:css": "node scripts/css-fetch-check.js",
    "test:mobile:gate": "node scripts/mobile_quality_gate.js",
    "test:router": "node scripts/chatbot-routing-check.js",
    "test:critical": "node --test tests-node/*.test.js",
    "dev": "npm run build:public && node scripts/local_server.js",
    "start": "node scripts/serve_static_ci.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "vercel": "^33.7.0"
  },
  "engines": {
    "node": "20.x"
  }
}
